#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/bin/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/bin/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/nanoid@5.1.6/node_modules:/home/khali/webspec-lang/webspec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/bin/nanoid.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/nanoid@5.1.6/node_modules/nanoid/bin/nanoid.js" "$@"
fi
